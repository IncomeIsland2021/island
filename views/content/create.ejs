<script>
var countval = 1; //for Increase id num  
function attribute_cloning_add(){
    var new_count = countval++; //for increase id number
    // both for change id  
    $clone1 = $("#attribute_section_add").clone();
    $clone1.attr('id','attribute_section_add'+new_count);
    $('#attribute_section_add_main').append($clone1); //for append Clone Data
    re_arrange_qualification();
    
}

</script>

<!-- Remove Clone by Click Minus Button -->
<script>
    function remove_current_div_venues(obj){
        var parent_id = obj.parent().parent().attr('id'); //for parent ID
        if( parent_id != 'attribute_section_add'){
            obj.parent().parent().remove();
            re_arrange_qualification(); 
        }    
    }

    function displayRadioValue(value) {
           
            if(value=="No"){
              $("#price-detail-usd").css("display","none")

               $("#price-detail-tab").css("display","none")
               $("#price-detail-bid").css("display","none")

            }
            else if(value=="Bid"){

              $("#price-detail-usd").css("display","none")

               $("#price-detail-tab").css("display","none")
               
              $("#price-detail-bid").css("display","")

            }
            else
            {
              $("#price-detail-usd").css("display","")

              $("#price-detail-tab").css("display","")
              
              $("#price-detail-bid").css("display","none")

            }
        }
  
        function checkForSale(value){

           let  total_copy=$("#total_copy").val();
           let total=parseInt(total_copy);
           if(value>total){
             alert("No.Of copy For Sale Should not be greater then Total Copy");
             $("#copy_for_sale").val("");
           }

        }
</script>
